hetero <- function(x) {
B_0 <- 0
B_1 <- 2
sigma = x^1.5
eps = rnorm(x,mean=0,sd=sqrt(sigma))
y = B_0 + B_1*x + eps
y
}
hetero(3)
hetero(4)
